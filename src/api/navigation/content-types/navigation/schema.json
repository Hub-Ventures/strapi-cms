{
    "kind": "collectionType",
    "collectionName": "navigations",
    "info": {
        "singularName": "navigation",
        "pluralName": "navigations",
        "displayName": "Navigation",
        "description": "Dynamic navigation menu items"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {
        "content-manager": {
            "visible": false
        }
    },
    "attributes": {
        "label": {
            "type": "string",
            "required": true,
            "maxLength": 100
        },
        "url": {
            "type": "string",
            "required": true,
            "maxLength": 255
        },
        "target": {
            "type": "enumeration",
            "enum": [
                "_self",
                "_blank"
            ],
            "default": "_self"
        },
        "order": {
            "type": "integer",
            "default": 0
        },
        "parent": {
            "type": "string",
            "maxLength": 100
        },
        "icon": {
            "type": "string",
            "maxLength": 50
        },
        "active": {
            "type": "boolean",
            "default": true
        }
    }
}