{
  "collectionName": "components_shared_nav_items",
  "info": {
    "displayName": "Nav Item",
    "icon": "link",
    "description": "Single navigation link for navbar or footer"
  },
  "attributes": {
    "label": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "url": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "target": {
      "type": "enumeration",
      "enum": ["_self", "_blank"],
      "default": "_self"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "icon": {
      "type": "string",
      "maxLength": 50
    }
  }
}
